{
	"name": "@sapphire/eslint-config",
	"version": "3.1.0",
	"description": "Standard ESLint config for Sapphire Communitys written in TypeScript",
	"author": "@sapphire",
	"license": "MIT",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		"import": "./dist/index.js",
		"require": "./dist/index.js"
	},
	"homepage": "https://github.com/sapphire-community/utilities/tree/main/packages/eslint-config",
	"scripts": {
		"test": "jest",
		"prelint": "yarn test && yarn build",
		"lint": "eslint src tests --ext ts --fix -c dist/index.js",
		"build": "tsc -b src",
		"prepublishOnly": "yarn build"
	},
	"dependencies": {
		"@typescript-eslint/eslint-plugin": "4.21.0",
		"@typescript-eslint/parser": "4.22.0",
		"eslint": "7.23.0",
		"eslint-config-prettier": "8.1.0",
		"eslint-plugin-prettier": "3.3.1",
		"prettier": "2.2.1",
		"typescript": "4.2.4"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sapphire-community/utilities.git",
		"directory": "packages/eslint-config"
	},
	"files": [
		"dist",
		"!dist/*.tsbuildinfo"
	],
	"engines": {
		"node": ">=12",
		"npm": ">=6"
	},
	"keywords": [
		"sapphire-community",
		"bot",
		"typescript",
		"ts",
		"yarn",
		"discord",
		"sapphire",
		"standalone"
	],
	"bugs": {
		"url": "https://github.com/sapphire-community/utilities/issues"
	},
	"publishConfig": {
		"access": "public"
	}
}
