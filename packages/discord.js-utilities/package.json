{
	"name": "@sapphire/discord.js-utilities",
	"version": "1.5.1",
	"description": "Discord.js specific utilities for your JavaScript/TypeScript bots",
	"author": "@sapphire",
	"license": "MIT",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		"import": "./dist/index.mjs",
		"require": "./dist/index.js"
	},
	"homepage": "https://github.com/sapphire-project/utilities/tree/main/packages/discord.js-utilities",
	"scripts": {
		"test": "echo \"\"",
		"lint": "eslint src tests --ext ts --fix -c ../../.eslintrc",
		"build": "rollup -c rollup.config.ts",
		"start": "yarn build -w",
		"prepublishOnly": "yarn build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sapphire-project/utilities.git",
		"directory": "packages/discord.js-utilities"
	},
	"files": [
		"dist",
		"!dist/*.tsbuildinfo"
	],
	"engines": {
		"node": ">=14",
		"npm": ">=6"
	},
	"keywords": [
		"@sapphire/discord.js-utilities",
		"bot",
		"typescript",
		"ts",
		"yarn",
		"discord",
		"sapphire",
		"standalone",
		"discord.js"
	],
	"bugs": {
		"url": "https://github.com/sapphire-project/utilities/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"@sapphire/discord-utilities": "^2.0.5"
	},
	"peerDependencies": {
		"discord.js": "^12.5.1"
	}
}
