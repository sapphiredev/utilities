{
	"name": "@sapphire/phisherman",
	"version": "1.4.1",
	"description": "Wrapper around phisherman to easily check and report domains",
	"author": "@sapphire",
	"license": "MIT",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"exports": {
		"import": "./dist/index.js",
		"require": "./dist/index.cjs",
		"types": "./dist/index.d.ts"
	},
	"homepage": "https://github.com/sapphiredev/utilities/tree/main/packages/phisherman",
	"scripts": {
		"lint": "eslint src --ext ts --fix",
		"build": "tsup",
		"docs": "typedoc-json-parser",
		"prepack": "yarn build",
		"bump": "cliff-jumper",
		"check-update": "cliff-jumper --dry-run"
	},
	"dependencies": {
		"@sapphire/fetch": "workspace:^"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sapphiredev/utilities.git",
		"directory": "packages/phisherman"
	},
	"files": [
		"dist/"
	],
	"engines": {
		"node": ">=16.6.0",
		"npm": ">=7.0.0"
	},
	"keywords": [
		"sapphiredev",
		"bot",
		"typescript",
		"ts",
		"yarn",
		"discord",
		"sapphire",
		"phisherman",
		"scam",
		"detection"
	],
	"bugs": {
		"url": "https://github.com/sapphiredev/utilities/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@favware/cliff-jumper": "^2.0.0",
		"tsup": "^6.7.0",
		"typedoc": "0.23.28",
		"typedoc-json-parser": "^7.2.0",
		"typescript": "^5.0.4"
	}
}
