{
	"name": "@sapphire/phisherman",
	"version": "2.0.1",
	"description": "Wrapper around phisherman to easily check and report domains",
	"author": "@sapphire",
	"license": "MIT",
	"main": "dist/cjs/index.cjs",
	"module": "dist/esm/index.mjs",
	"types": "dist/cjs/index.d.cts",
	"exports": {
		"import": {
			"types": "./dist/esm/index.d.mts",
			"default": "./dist/esm/index.mjs"
		},
		"require": {
			"types": "./dist/cjs/index.d.cts",
			"default": "./dist/cjs/index.cjs"
		}
	},
	"homepage": "https://github.com/sapphiredev/utilities/tree/main/packages/phisherman",
	"scripts": {
		"lint": "eslint src --ext ts --fix",
		"build": "yarn gen-index && tsup && yarn build:rename-cjs-index",
		"build:rename-cjs-index": "tsx ../../scripts/rename-cjs-index.cts",
		"docs": "typedoc-json-parser",
		"prepack": "yarn build",
		"bump": "cliff-jumper",
		"check-update": "cliff-jumper --dry-run",
		"gen-index": "tsx ../../scripts/gen-index.cts -w phisherman"
	},
	"dependencies": {
		"@sapphire/fetch": "workspace:^"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sapphiredev/utilities.git",
		"directory": "packages/phisherman"
	},
	"files": [
		"dist/"
	],
	"engines": {
		"node": ">=18"
	},
	"keywords": [
		"sapphiredev",
		"bot",
		"typescript",
		"ts",
		"yarn",
		"discord",
		"sapphire",
		"phisherman",
		"scam",
		"detection"
	],
	"bugs": {
		"url": "https://github.com/sapphiredev/utilities/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@favware/cliff-jumper": "^4.0.2",
		"@sapphire/result": "workspace:^",
		"esbuild-plugin-version-injector": "^1.2.1",
		"tsup": "^8.1.0",
		"tsx": "^4.15.7",
		"typedoc": "^0.25.13",
		"typedoc-json-parser": "^10.0.0",
		"typescript": "^5.5.2"
	}
}
